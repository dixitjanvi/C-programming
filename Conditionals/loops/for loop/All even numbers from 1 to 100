//Print all the even numbers from 1 to 100
#include<stdio.h>
int main(){
    printf("Even numbers:\n");
    for(int i=1; i<=100; i++){
        if(i%2==0){
            printf("%d\n",i);
        }
    }
    return 0;
}



//Method-2
#include<stdio.h>
int main(){
    printf("Even numbers:\n");
    for(int i=2; i<=100; i=i+2){
            printf("%d\n",i);
    }
    return 0;
}
