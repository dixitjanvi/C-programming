//Take positive integer input and tell if it is divisible by 5 and 3.
#include<stdio.h>
int main(){
    int x;
    printf("Enter a number:");
    scanf("%d",&x);
    
    //== it's use for comparision
    if(x%5==0 & x%3==0){
        printf("This number is divisible by 3 and 5 both.");
    }
    else{
        if(x%5==0){
            printf("This number is divisible by only 5.");
        }
        else{
            if(x%3==0){
                printf("This number is divisible by only 3.");
            }
            else{
                printf("This number is not divisible by 3 and 5 both.");
            }
        }
    }
    return 0;
}
