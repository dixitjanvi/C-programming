//WAP to check if input year is leap year.
#include<stdio.h>
int main(){
    int y;
    printf("Enter a year:");
    scanf("%d",&y);
    
    //Condition ? True : False;
    ((y%400==0) || y%4==0 & y%100!=0) ? printf("These year is a leap year.") : printf("These year is not a leap year.");
    return 0;
}
