// Take positive integer input and tell if it is
// divisible by 5 and 3 but not divisible by 15.


#include <stdio.h>
int main(){
    int n;
    printf("Enter a number:");
    scanf("%d",&n);
    if((n%3==0  || n%5==0) && n%15!=0){
          printf("these interger is divisible by both but not divisible by 15");
    }
    else{
        printf("these integer divisible by 3,5 and 15");
    }
    return 0;
}
