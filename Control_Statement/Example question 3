// Take input percentage of a student and print the grade according to marks.
// 1)90-100 Excellent 2)80-90 Very Good 3)70-80 Good
// 4)60-70 Can do better 5)50-60 Average
// 6)40-50 Below average 7)<40 Fail


#include <stdio.h>
int main()
{
    int n;
    printf("Enter a percentage:");
    scanf("%d", &n);
    if(n>90){
        printf(" Excellent!");
    }
    else if(n>80){
        printf("Very Good ");
    }
    else if(n>70){
        printf(" Good ");
    }
    else if(n>60){
        printf("Can do better ");
    }
    else if(n>50){
        printf("Average ");
    }
    else if(n>40){
        printf("Below Average ");
    }
    else{
        printf("Fail");
    }
        return 0;
}
