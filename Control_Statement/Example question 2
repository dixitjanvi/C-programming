// If the ages of ram,shyam and ajay are input through the
// keyword ,write a program to datermine the youngest of the three.


#include <stdio.h>
int main(){
    int ram, shyam, ajay;
    printf("Enter a ram age:");
    scanf("%d",&ram);
    printf("Enter a shyam age:");
    scanf("%d",&shyam);
    printf("Enter a ajay age:");
    scanf("%d",&ajay);
    if(ram>shyam){
        if(ram>ajay){
        printf("ram is greatest");
        }
        else{
            printf("ajay is greatest");
        }
    }
    else{
        if(shyam>ajay){
        printf("shyam is greatest");
        }
        else{
            printf("ajay is greatest");
        }
    }
    return 0;
}
