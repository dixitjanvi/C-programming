#include <stdio.h>
int main(){
    float maths = 37;     // , and blank space are not allowed in variable name
    float physics = 35;
    float chemistry = 38;
    float english = 37;
    float percentage = (maths + physics + chemistry + english) /160*100;
    printf("Percentage of 4 subjects is : %f",percentage);
    return 0;
} 
